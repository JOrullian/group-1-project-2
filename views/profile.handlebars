<div class="saved-events-container">
    <div class="saved-events-header">
        <h1 class="profile-title">User Profile</h1>

    </div>
    <div class="division-bar"></div>
    <div class="saved-events-list">
        <p class="profile-information" id="profile-username">UserName: {{username}}</p>
        </br>
        </br>
        <p class="profile-information" id="profile-email">Email: {{email}}</p>
        </br>
        </br>
        <p class="profile-information" id="profile-firstname">First Name: {{firstname}}</p>
        </br>
        </br>
        <p class="profile-information" id="profile-location">Preferred Location: {{location}}</p>
        </br>
        </br>
        <p class="profile-information" id="profile-preferred-sport">Preferred Sport: {{preferred_sport}}</p>
        </br>
        </br>
        <p class="profile-information" id="profile-play_frequency">How often do you play?: {{play_frequency}}</p>
        </br>
        </br>
        <div class="profile-logout"></div>
        <button id="logout-btn" class="logout-btn">Logout</button>
    </div>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
</div>